<config version='1.0'>
   <scope>
      <desc>
			menubar component : Extends the menubar component to display the
			 new custom menu option
      </desc>
	  <!-- Component Contract -->
	  <!--<component id="menubar" extends="menubar:webtop/config/menubar_component.xml">-->
      <component id="menubar" extends="menubar:wp/config/app/menubar_component.xml">

		<!-- Component Layout -->     
         <pages>
            <start>/custom/app/custom_menubar.jsp</start>
         </pages>
         
		 <!-- NLS properties file -->
         <nlsbundle>com.newsarticle.app.WpMenuBarNlsProp</nlsbundle>
      </component>
      
   </scope>
</config>

